unit UCIDADES;

interface

uses
  Classes;

type
  TCIDADES = class(TPersistent)
  private
   FCODIGO: String;
   FCIDADE: String;
   FUF: String;
  published
   property CODIGO: String read FCODIGO write FCODIGO;
   property CIDADE: String read FCIDADE write FCIDADE;
   property UF: String read FUF write FUF;
  end;

implementation

end.